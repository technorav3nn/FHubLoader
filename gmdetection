nouniversal=false
local function ld(l)
loadstring(game:HttpGet(l,true))()
nouniversal=true

if game.PlaceId==4958040666 then
ld("https://raw.githubusercontent.com/technorav3nn/FHubLoader/main/SCLoader")
end

if game.PlaceId==1188096566 then
ld("https://raw.githubusercontent.com/technorav3nn/FHubLoader/main/ClubDJLoader")
-- use individual club dj loader above pls.
end

if nouniversal==false then
warn("game not loaded, loading Owl Hub Universal FPS.")
ld("https://raw.githubusercontent.com/SiLeNSwOrD/OwlHub/master/OwlHub.txt")
-- Im making my own universal FPS script soon, using oh's esp lib. -- 
end


